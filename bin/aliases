alias mysql='mysql --silent'
alias pause='playlist pause-after-current'
alias pdb='python3 -m pdb'
alias pip3i='pip3 install'
alias png2gif='convert -delay 10 -loop 0 *.png animaion.gif'
alias py='python3 -q'
alias rot13='caesar --offset=13'
alias rt='t retweet'
alias sleep='sleeptill --relative'
alias srsly='retry'
alias sssel='ssel --sudo'
alias ssid='airport --getinfo | pcregrep -o1 " SSID: (.*)"'
alias ssync='rsync --rsh=ssh'
alias sudo='sudo -p '"'"'[sudo] Password for %p:'"'"' '
alias tf='tail -fn0'
alias unixdate='command date -u "+[date] %s"'
alias whatismyip='curl ifconfig.me'
