alias android-studio='open -a "Android Studio"'
alias cargo-png='cargo graph | dot -Tpng'
alias dacapo='playlist repeat-current-once'
alias fav='t fave'
alias gp='git pull --rebase --autostash'
alias http='python3 -m http.server'
alias inet='syncbin hasinet'
if [ -d /opt/git/github.com/jq-shell/jqsh/master ]; then
    alias jqsh='cd /opt/git/github.com/jq-shell/jqsh/master && git pull && rustup --quiet --release && cd - > /dev/null && command jqsh'
elif [ -d "$HOME"/git/github.com/jq-shell/jqsh/master ]; then
    alias jqsh='cd '"$HOME"'/git/github.com/jq-shell/jqsh/master && git pull && rustup --quiet --release && cd - > /dev/null && command jqsh'
fi
alias mysql='mysql --silent'
alias pause='playlist pause-after-current'
alias pdb='python3 -m pdb'
alias png2gif='convert -delay 10 -loop 0 *.png animation.gif'
alias py='python3 -q'
alias rot13='caesar --offset=13'
alias rt='t retweet'
alias sleep='sleeptill --relative'
alias sssel='ssel --sudo'
alias ssid='airport --getinfo | pcregrep -o1 " SSID: (.*)"'
alias ssync='rsync --rsh=ssh'
alias tf='tail -fn0'
alias unixdate='command date -u "+%s"'
alias whatismyip='curl ifconfig.me'
